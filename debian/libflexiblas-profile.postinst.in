#! /bin/sh

set -e

if [ "`lsb_release -r -s`" = "12.04" ]
then
	update-alternatives --install /usr/lib/libblas.so.3 libblas.so.3 /usr/lib/@DEB_HOST_MULTIARCH@/flexiblas-profile/update-alternatives/libblas.so.3 35 
	update-alternatives --install /usr/lib/libblas.so.3gf libblas.so.3gf /usr/lib/@DEB_HOST_MULTIARCH@/flexiblas-profile/update-alternatives/libblas.so.3 35 
else
	update-alternatives --install /usr/lib/libblas.so.3 libblas.so.3 /usr/lib/@DEB_HOST_MULTIARCH@/flexiblas-profile/update-alternatives/libblas.so.3 35 \
	--slave /usr/lib/libblas.so.3gf libblas.so.3gf /usr/lib/@DEB_HOST_MULTIARCH@/flexiblas-profile/update-alternatives/libblas.so.3 
fi 

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0


